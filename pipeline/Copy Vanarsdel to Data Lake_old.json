{
	"name": "Copy Vanarsdel to Data Lake_old",
	"properties": {
		"activities": [
			{
				"name": "ForEach_to1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_to1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "dbo.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "sr-blob-container-dev/raw/vanarsdel/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "ParquetSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "ParquetWriteSettings"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_to1",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_to1",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "Actors"
						},
						"destination": {
							"fileName": "dboActors"
						}
					},
					{
						"source": {
							"table": "Customers"
						},
						"destination": {
							"fileName": "dboCustomers"
						}
					},
					{
						"source": {
							"table": "MovieActors"
						},
						"destination": {
							"fileName": "dboMovieActors"
						}
					},
					{
						"source": {
							"table": "Movies"
						},
						"destination": {
							"fileName": "dboMovies"
						}
					},
					{
						"source": {
							"table": "OnlineMovieMappings"
						},
						"destination": {
							"fileName": "dboOnlineMovieMappings"
						}
					}
				]
			}
		},
		"annotations": []
	}
}