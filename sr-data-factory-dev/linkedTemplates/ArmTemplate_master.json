{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sr-data-factory-dev"
		},
		"CloudSalesDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudSalesDB'"
		},
		"CloudStreamingDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CloudStreamingDB'"
		},
		"Fourth Coffee_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Fourth Coffee'"
		},
		"MoviesDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MoviesDB'"
		},
		"srlakedev_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'srlakedev'"
		},
		"Fourth Coffee_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Rentals"
		},
		"Fourth Coffee_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "southridge"
		},
		"srlakedev_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://srlakedev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sr-data-factory-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CloudSalesDB_connectionString": {
						"value": "[parameters('CloudSalesDB_connectionString')]"
					},
					"CloudStreamingDB_connectionString": {
						"value": "[parameters('CloudStreamingDB_connectionString')]"
					},
					"MoviesDB_connectionString": {
						"value": "[parameters('MoviesDB_connectionString')]"
					},
					"srlakedev_accountKey": {
						"value": "[parameters('srlakedev_accountKey')]"
					},
					"Fourth Coffee_password": {
						"value": "[parameters('Fourth Coffee_password')]"
					},
					"srlakedev_properties_typeProperties_url": {
						"value": "[parameters('srlakedev_properties_typeProperties_url')]"
					},
					"Fourth Coffee_properties_typeProperties_host": {
						"value": "[parameters('Fourth Coffee_properties_typeProperties_host')]"
					},
					"Fourth Coffee_properties_typeProperties_userId": {
						"value": "[parameters('Fourth Coffee_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sr-data-factory-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sr-data-factory-dev_ArmTemplate_0')]"
			]
		}
	]
}